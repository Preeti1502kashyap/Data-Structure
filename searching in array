#include<iostream>
using namespace std;
int main()
{
    int i,n,s,p;
    cout<<"enter the size of array:";
    cin>>n;
    int arr[n];
    cout<<"enter the elements of array:";
    for(i=1;i<=n;i++)
    {
        cin>>arr[i];

    }
    cout<<" traversing of array:";
    for(i=1;i<=n;i++)
    {
        cout<<arr[i];

    }
    cout<<"enter the element to search:";
    cin>>s;
    for(i=1;i<=n;i++)
    {

        if(s==arr[i])
        {

            p=i;

        }
    }
    if(p>0)
    {

        cout<<"\n element at:"<<p;

    }
    else{
        cout<<"\n element is not there:";
    }

    return 0;
}
