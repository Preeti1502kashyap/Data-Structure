#include<iostream>
using namespace std;
int main()
{
    int i,n;
    cout<<"enter the size of array:";
    cin>>n;
    int arr[n];
    cout<<"enter the elements of array:"<<endl;
    for(i=0;i<=n;i++)
    {
        cin>>arr[i];

    }
    cout<<" traversing of array:";
    for(i=0;i<=n;i++)
    {
        cout<<"arr["<<i<<"]:"<<arr[i]<<endl;

    }
}
